{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es6",
        "jsx": "react-jsx",
        "allowJs": true,
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "Node",
        "types": ["@emotion/react/types/css-prop", "node"],
        "lib": ["es2020", "dom"],
        "baseUrl": "src"
    }
}
